    "a = 1\n",
    "b = 1\n",
    "c = -6\n",
    "x = -2\n",
    "y = a * x ** 2 + b * x + c\n",
    "print('y calculado no ponto x=-2 é igual a', y)\n",
    "x = 0 \n",
    "y = a * x ** 2 + b * x + c\n",
    "print('y calculado no ponto x=0 é igual a', y)\n",
    "x = 2\n",
    "y = a * x ** 2 + b * x + c\n",
    "print('y calculado no ponto x=2 é igual a', y)"
